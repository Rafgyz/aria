func greater_or_equal(x,y) {
    return x >= y;
}

func main() {
    assert greater_or_equal(1.0f, 1.0f);
    assert greater_or_equal(1.0f, 0);
    assert greater_or_equal(1, 0.1f);

    assert !greater_or_equal(1.1f, 3.14f);
    assert !greater_or_equal(2, 3.14f);
    assert !greater_or_equal(3.14f, 5);
}
