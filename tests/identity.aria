func main() {
    val x = 123;
    val y = "hello";
    val z = false;

    assert identity(x) != 0;
    assert identity(y) != 0;
    assert identity(z) != 0;

    assert identity(x) != identity(y);
    assert identity(z) != identity(y);
    assert identity(x) != identity(z);
}
